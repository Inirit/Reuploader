{
    "manifest_version": 2,
    "name": "Reuploader",
    "version": "1.0.0prealpha",
    "default_locale": "en",
    "description": "A web extension for quickly and easily rehosting images.",
    "developer": {
        "name": "Inirit",
        "url": "https://github.com/Inirit/Reuploader"
    },
    "permissions": [
        "<all_urls>",
        "contextMenus",
        "clipboardWrite"
    ],
    "background": {
        "scripts": [
            "/node_modules/jquery/dist/jquery.js",
            "/node_modules/systemjs/dist/system.js",
            "js/systemBootstrap.js",
            "js/background.js",
            "js/uploaders/pomfUploader.js"
        ]
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}